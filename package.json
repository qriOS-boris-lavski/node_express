{
  "name": "http_crud",
  "version": "1.0.0",
  "description": "http server with CRUD for accounts and token generation",
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist/ && npx tsc",
    "start": "node dist/index.js",
    "tslint": "npx tslint -c tslint.json -p tsconfig.json",
    "test": "npx jest --coverage --forceExit --detectOpenHandles",
    "tslint-fix": "npx tslint -c tslint.json -p tsconfig.json --fix",
    "buildAll": "yarn cache clean && npx rimraf node_modules/ && npx rimraf yarn.lock && yarn && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qriOS-boris-lavski/node_express.git"
  },
  "author": "Vasyl Boryslavskyi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/qriOS-boris-lavski/node_express/issues"
  },
  "homepage": "https://github.com/qriOS-boris-lavski/node_express#readme",
  "dependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "mysql2": "^3.4.0",
    "sequelize": "^6.32.1"
  }
}
